<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cl.ingenieriasantafe.gerenciapp">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icono"
        android:label="@string/app_name"
        android:usesCleartextTraffic="true"
        android:roundIcon="@drawable/icono"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MenuDrawer_Activity"
            android:label="@string/title_activity_menu_drawer_"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".Combustibles_detalle_asignados_Activity"
            android:label="@string/title_activity_menu_drawer_"
            android:parentActivityName=".MenuDrawer_Activity">
        </activity>
        <activity
            android:name=".Precios_copec_Activity"
            android:label="@string/title_activity_menu_drawer_">
        </activity>

        <activity
            android:name=".detalle_combustible_consumidos_unegocio_Activity"
            android:label="@string/title_activity_menu_drawer_">
        </activity>

        <activity
            android:name=".Pruebas_Activity"
            android:label="@string/title_activity_menu_drawer_">
        </activity>
        <activity
            android:name=".Combustibles_pedidos_detalle_activity"
            android:label="@string/title_activity_menu_drawer_">
        </activity>




    </application>

</manifest>